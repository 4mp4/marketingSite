{
  "name": "@cirrica/marketingsite-dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",    
    "build:worker": "opennextjs-cloudflare build",
    "deploy:dev": "npm run build:worker && npx wrangler deploy --env dev",
    "deploy:prod": "npm run build:worker && npx wrangler deploy --env prod",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.1.0",
    "framer-motion": "^12.15.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "wrangler": "^4.18.0"
  }
}
